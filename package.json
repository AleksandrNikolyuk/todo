{
	"name": "todo",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@material-ui/core": "^4.6.1",
		"classnames": "^2.2.6",
		"i18next": "^19.0.0",
		"i18next-browser-languagedetector": "^4.0.1",
		"prop-types": "^15.7.2",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-i18next": "^11.2.2",
		"react-native": "^0.61.5",
		"react-redux": "^7.1.3",
		"react-scripts": "3.2.0",
		"react-scrollbar": "^0.5.6",
		"redux": "^4.0.4",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"uniqid": "^5.1.0"
	},
	"devDependencies": {
		"nyc": "^14.1.1",
		"eslint-plugin-material-ui": "^1.0.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"cover": "nyc npm run test",
		"check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"nyc": {
		"include": [
			"src/**/*.js"
		],
		"exclude": [
			"**/*.spec.js"
		],
		"reporter": [
			"lcov",
			"text",
			"html"
		]
	}
}
